<UserControl x:Class="handovermgr.Controls.PatametersListUI"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:handovermgr.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <StackPanel x:Name="ParametersList" Orientation="Vertical" Grid.Column="0">

        <Label Content="Parameters"  Margin="0 0 0 5" FontWeight="SemiBold"/>
        <Label Content="Throughoutput:"/>
        <Label Content="BER:"/>
        <Label Content="Burst Error Rate:"/>
        <Label Content="Packet Loss:"/>
        <Label Content="Delay:"/>
        <Label Content="Response:"/>
        <Label Content="Jitter:"/>
        <Label Content="Security:"/>
        <Label Content="Cost:"/>
    </StackPanel>
</UserControl>
